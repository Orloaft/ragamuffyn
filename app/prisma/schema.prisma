generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model character {
  id      String  @id
  name    String?
  data    String?
}

model npc {
    id     String    @id 
  name    String?
  data    String?
}

model item {
   id      String     @id 
    name    String?
  data     String?
}

model campaign {
   id      String     @id 
  name    String? 
  data    String?
}


model encounter {
   id      String     @id 
    name    String?
  data     String?
}

model location {
   id      String     @id 
  name    String? 
  data    String?
}